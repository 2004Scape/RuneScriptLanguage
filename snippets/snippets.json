{
    "Chat Npc": {
        "prefix": "chatnpc",
        "body": "~chatnpc(\"<p,${1|happy,bored,short,neutral,shock,confused,silent,quiz,default,shifty,worried,drunk,verymad,laugh,madlaugh,sad,angry|}>${2:text}\");",
