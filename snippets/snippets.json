    "Label": {
        "prefix": "label",
        "body": [
            "[label,${1:label_name}]",
            "$2"            
        ],
        "scope": "runescript",
        "description": "A shortcut creating a new label."
    },
    "Proc": {
        "prefix": "proc",
        "body": [
            "[proc,${1:proc_name}]",
            "$2"            
        ],
        "scope": "runescript",
        "description": "A shortcut for creating a new proc."
    },
    "Label with Params": {
        "prefix": "label (params)",
        "body": [
            "[label,${1:label_name}](${2:params})",
            "$3"            
