        "description": "A shortcut creating a new label with params."
    },
    "Proc with params": {
        "prefix": "proc (params)",
        "body": [
            "[proc,${1:proc_name}](${2:params})(${3:return_value})",
            "$4"            
        ],
        "scope": "runescript",
        "description": "A shortcut for creating a new proc with params."
    },
    "Drops": {
        "prefix": "drop",
        "body": [
            "[ai_queue3,${1:npc}]",
            "gosub(npc_death);",            
            "if (npc_findhero = false) {",
            "\treturn;",
            "}",
            "// Default drop from config.",
            "obj_add(npc_coord, npc_param(death_drop), 1, ^lootdrop_duration);",
            "",
            "def_int $$random = random(${2:roll});",
            "",
            "if ($$random < ${3:chance}) {",
            "\tobj_add(npc_coord, ${4:drop}, 1, ^lootdrop_duration);",
            "}"
        ],
        "scope": "runescript",
        "description": "Generate the skeleton for drops file."
    },
    "Give Xp": {
        "prefix": "givexp",
        "body": "givexp(${1|attack,defence,strength,hitpoints,ranged,prayer,magic,woodcutting,fletching,fishing,firemaking,crafting,smithing,mining,herblore,agility,thieving,runecraft|}, ${2:100});",
        "scope": "runescript",
        "description": "A shortcut for givexp command."
    }
