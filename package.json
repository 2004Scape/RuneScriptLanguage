{
    "name": "runescriptlanguage",
    "displayName": "RuneScript Language Support",
    "publisher": "2004scape",
    "repository": "https://github.com/2004scape/RuneScriptLanguage",
    "description": "Syntax highlighting for RuneScript.",
    "version": "0.0.4",
    "engines": {
        "vscode": "*"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "runescript",
                "aliases": [
                    "RuneScript",
                    "Runescript",
                    "runescript"
                ],
                "extensions": [
                    ".rs2"
                ],
                "configuration": "./runescript.language-configuration.json"
            },
            {
                "id": "confscript",
                "aliases": [
                    "ConfScript",
                    "Confscript",
                    "confscript"
                ],
                "extensions": [
                    ".constant",
                    ".dbrow",
                    ".dbtable",
                    ".enum",
                    ".hunt",
                    ".idk",
                    ".if",
                    ".inv",
                    ".loc",
                    ".mesanim",
                    ".npc",
                    ".param",
                    ".seq",
                    ".struct",
                    ".varn",
                    ".varp"
                ],
                "configuration": "./confscript.language-configuration.json"
            },
            {
                "id": "objconfig",
                "aliases": [
                    "Object Config"
                ],
                "extensions": [
                    ".obj"
                ],
                "configuration": "./objconfig.language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "runescript",
                "scopeName": "source.runescript",
                "path": "./syntaxes/runescript.tmLanguage.json"
            },
            {
                "language": "confscript",
                "scopeName": "source.confscript",
                "path": "./syntaxes/confscript.tmLanguage.json"
            },
            {
                "language": "objconfig",
                "scopeName": "source.objconfig",
                "path": "./syntaxes/objconfig.tmLanguage.json"
            }
        ]
    }
}